# =============================================================================
# Real Estate Multi-Agent System - Environment Configuration
# =============================================================================
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to git!

# =============================================================================
# API KEYS
# =============================================================================
GROQ_API_KEY=your_groq_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here
TAVILY_API_KEY=your_tavily_api_key_here

# =============================================================================
# DATABASE CONFIGURATION
# =============================================================================
# PostgreSQL
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
POSTGRES_DB=real_estate_db
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_secure_password_here

# Database URL (auto-constructed from above)
DATABASE_URL=postgresql://${POSTGRES_USER}:${POSTGRES_PASSWORD}@${POSTGRES_HOST}:${POSTGRES_PORT}/${POSTGRES_DB}

# =============================================================================
# VECTOR DATABASE (QDRANT)
# =============================================================================
QDRANT_HOST=qdrant
QDRANT_PORT=6333
QDRANT_URL=http://${QDRANT_HOST}:${QDRANT_PORT}

# =============================================================================
# EMBEDDING MODEL
# =============================================================================
EMBEDDING_MODEL=sentence-transformers/all-mpnet-base-v2
EMBEDDING_DIMENSION=768

# =============================================================================
# LLM CONFIGURATION
# =============================================================================
# Primary LLM
LLM_PROVIDER=groq
LLM_MODEL=llama-3.1-70b-versatile
LLM_TEMPERATURE=0.1

# Backup LLM
BACKUP_LLM_PROVIDER=gemini
BACKUP_LLM_MODEL=gemini-1.5-flash

# =============================================================================
# MODEL PATHS (Inside Container)
# =============================================================================
CHECKPOINT_PATH=/app/models/floorplan_checkpoint.pth
ROOM_CLASSIFIER_PATH=/app/models/room_classifier.pkl

# =============================================================================
# DATA PATHS (Inside Container)
# =============================================================================
IMAGES_DIR=/app/data/images
CERTIFICATES_DIR=/app/data/certificates
REPORTS_DIR=/app/data/reports
EXCEL_FILE=/app/data/Property_list.xlsx

# =============================================================================
# FLOORPLAN PARSER CONFIGURATION
# =============================================================================
FLOORPLAN_CONFIDENCE_THRESHOLD=0.5
ML_CONFIDENCE_THRESHOLD=0.6

# =============================================================================
# APPLICATION SETTINGS
# =============================================================================
BACKEND_PORT=8000
FRONTEND_PORT=8501
DEBUG=false
LOG_LEVEL=INFO

# =============================================================================
# CORS SETTINGS
# =============================================================================
ALLOWED_ORIGINS=http://localhost:8501,http://localhost:3000

# =============================================================================
# COLLECTION NAMES
# =============================================================================
PROPERTY_COLLECTION=property_embeddings
CERTIFICATE_COLLECTION=certificate_embeddings